  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      // "program": "${workspaceFolder}/main/index.ts",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
      "windows": {
        "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd"
      },
      "outFiles": [
        "${workspaceRoot}/dist/index.js"
      ],
      "outputCapture": "std",
      "args": [
        "."
      ]
    }
  ]
}